{
  "name": "dicoding-grave-discover-backend",
  "version": "1.0.0",
  "main": "./src/index.js",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "start-dev": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint ./src",
    "install:fe": "cd frontend && yarn",
    "start:fe": "cd frontend && yarn start-dev",
    "build:fe": "cd frontend && yarn build",
    "dev": "npx nodemon",
    "bundle": "cd frontend && npx webpack --config client/webpack.config.js",
    "table:status": "npx knex migrate:status",
    "table:clear": "npx knex migrate:rollback",
    "table:build": "npx knex migrate:latest",
    "table:up": "npx knex migrate:up",
    "table:down": "npx knex migrate:down",
    "reset": "npx cross-env rm -rf ./node_modules && yarn",
    "start": "node ./src"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "bootstrap": "^5.3.0-alpha3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.3.1",
    "regenerator-runtime": "^0.13.11"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "nodemon": "^2.0.22",
    "postcss-loader": "^7.3.2",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.9.0",
    "yarn": "^1.22.19"
  }
}
