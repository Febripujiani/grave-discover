{
  "name": "dicoding-grave-discover-backend",
  "version": "1.0.0",
  "main": "./src/index.js",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "dev": "npx nodemon",
    "bundle": "cd frontend && npx webpack --config client/webpack.config.js",
    "table:status": "npx knex migrate:status",
    "table:clear": "npx knex migrate:rollback",
    "table:build": "npx knex migrate:latest",
    "table:up": "npx knex migrate:up",
    "table:down": "npx knex migrate:down",
    "reset": "npx cross-env rm -rf ./node_modules && yarn",
    "start": "node ./src"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.3.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.22",
    "yarn": "^1.22.19"
  }
}
